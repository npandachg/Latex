\newcommand{\countU}[1]{\bigcup\limits_{i\in{\Zplus}}{#1}_{i}}
\newcommand{\indxU}[2]{\bigcup\limits_{\alpha\in{#1}}{#2}_{\alpha}}
\newcommand{\countDisjU}[1]{\disjU\limits_{i\in{\Zplus}}{#1}_{i}}
\newcommand{\finiteDisjU}[2]{\disjU\limits_{i=1}^{#2}{#1}}
\newcommand{\finiteUComp}[2]{\bigcup\limits_{i=1}^{#2}\indxComp{#1}{i}}
\newcommand{\countUComp}[1]{\bigcup\limits_{i\in\Zplus}\indxComp{#1}{i}}
\newcommand{\indxUComp}[2]{\bigcup\limits_{\alpha\in{#1}}\indxComp{#2}{\alpha}}
\newcommand{\countDisjUComp}[1]{\disjU\limits_{i\in{\Zplus}}\indxComp{#1}{i}}
% Intersections
\newcommand{\intersection}[2]{{#1}\cap{#2}}
\newcommand{\finiteI}[2]{\bigcap\limits_{i=1}^{#2}{#1}_{i}}
\newcommand{\countI}[1]{\bigcap\limits_{i\in{\Zplus}}{#1}_{i}}
\newcommand{\indxI}[2]{\bigcap\limits_{\alpha\in{#1}}{#2}_{\alpha}}
\newcommand{\countDisjI}[1]{\dot{\bigcap}\limits_{i\in{\Zplus}}{#1}_{i}}
\newcommand{\finiteIComp}[2]{\bigcap\limits_{i=1}^{#2}\indxComp{#1}{i}}
\newcommand{\countIComp}[1]{\bigcap\limits_{i\in\Zplus}\indxComp{#1}{i}}
\newcommand{\indxIComp}[2]{\bigcap\limits_{\alpha\in{#1}}\indxComp{#2}{\alpha}}
\newcommand{\countDisjIComp}[1]{\dot{\bigcap}\limits_{i\in{\Zplus}}\indxComp{#1}{i}}
% sums and products
\newcommand{\sumInf}[1]{\sum\limits_{i = 1}^{\infty}{#1}}
\newcommand{\sumFinite}[2]{\sum\limits_{i = 1}^{#2}{#1}}
